{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots":  ["./back-end/@types", "./node_modules/@types"]
  },
  "include": ["./back-end/**/*.ts"],
  "exclude": ["node_modules", "front-end"]
}
